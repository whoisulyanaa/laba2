import re
s66chet4 = []
for i in open('lab1.txt').read().split():
    if re.fullmatch(r'6.*6\d|(?=.{5,})(?=.*[02468]$)', i): s66chet4.append(i)  # Первая и предпоследняя цифра 6 или длина > 4 и четный последний символ
print(f"числа, у которых первая и предпоследняя цифра 6 и четные числа длиной более 4 символов: {s66chet4}")

